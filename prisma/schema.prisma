// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model books {
  id    Int    @id @default(autoincrement())
  title String
  author String
  genre String
  price Float
}